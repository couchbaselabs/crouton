#include "Project.xcconfig"

GCC_PREPROCESSOR_DEFINITIONS = NDEBUG=1 $(GCC_PREPROCESSOR_DEFINITIONS)

LIBRARY_SEARCH_PATHS         = $(inherited)  $(PROJECT_DIR)/build_cmake/release/xcodedeps/

RUN_CLANG_STATIC_ANALYZER    = YES
CLANG_STATIC_ANALYZER_MODE   = deep

DEAD_CODE_STRIPPING          = YES
DEBUG_INFORMATION_FORMAT     = dwarf-with-dsym
ENABLE_NS_ASSERTIONS         = NO
LLVM_LTO                     = YES

ONLY_ACTIVE_ARCH             = NO

// https://www.emergetools.com/blog/posts/how-xcode14-unintentionally-increases-app-size
DEPLOYMENT_POSTPROCESSING    = YES
STRIP_INSTALLED_PRODUCT      = YES
STRIPFLAGS                   = -rSTx

VALIDATE_PRODUCT = YES
